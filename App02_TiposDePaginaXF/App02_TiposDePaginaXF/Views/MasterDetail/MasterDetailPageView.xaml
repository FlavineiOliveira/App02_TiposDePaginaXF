<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App02_TiposDePaginaXF.Views.MasterDetail.MasterDetailPageView"
             xmlns:local="clr-namespace:App02_TiposDePaginaXF.Views.MasterDetail">
    <MasterDetailPage.Master>
        <ContentPage Title="Menu">
            <StackLayout>
                <Button Text="Página 1" BackgroundColor="Transparent" Clicked="Pagina1_Clicked" />
                <Button Text="Página 2" BackgroundColor="Transparent" Clicked="Pagina2_Clicked" />
                <Button Text="Início" BackgroundColor="Transparent" Clicked="Conteudo_Clicked" />
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <local:CounteudoMasterView />
    </MasterDetailPage.Detail>
</MasterDetailPage>